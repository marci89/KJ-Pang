
type = wallFrameType.Blue;


function SetType() {
	
	if(type == wallFrameType.Red)
		return wallType.Red;
		
	if(type == wallFrameType.Blue)
		return wallType.Blue;

	if(type == wallFrameType.Green)
		return wallType.Green;
		
	if(type == wallFrameType.Yellow)
		return wallType.Yellow;
		
	if(type == wallFrameType.Gray)
		return wallType.Gray;
		
	if(type == wallFrameType.Purple)
		return wallType.Purple;	
}

#endregion

